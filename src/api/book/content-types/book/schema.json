{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "BookTitle": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "OriginalTitle": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::original-book.original-book"
    },
    "BookSetTitle": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book-set.book-set",
      "inversedBy": "books"
    },
    "BookThumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "BookColour": {
      "type": "string"
    },
    "Content": {
      "type": "json",
      "required": true
    },
    "BookPackage": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
